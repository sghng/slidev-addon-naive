{
  "proseWrap": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-sh"],
  "importOrder": ["^node", "^vue", "^@slidev", "^naive-ui", "^[^.]", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": ["slides.md"],
      "options": {
        "parser": "slidev",
        "plugins": ["prettier-plugin-slidev"]
      }
    }
  ]
}
